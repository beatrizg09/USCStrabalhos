 public class Main
{
	public static void main(String[] args) {
		boolean x = 50 != 7;
		boolean y = true ==(1<=0);
		if (x==y)
		  System.out.println("USCS");
		  else {
		      if(x!=y)
		      System.out.println("Comp");
		  else 
		     System.out.println("algoritimo");
		  }
	}
